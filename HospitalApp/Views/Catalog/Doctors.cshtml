@model IEnumerable<HospitalApp.Models.Doctor>
@{
  
    ViewData["Title"] = "Bác sĩ";
}
<h2>@ViewData["Title"]</h2>
<div class="row g-3">
    @foreach (var d in Model)
    {
        <div class="col-md-4">
            <div class="card h-100">
                <div class="card-body">
                    <h5>@d.FullName</h5>
                    <div class="small text-muted">Mã BS: @d.Id</div>
                </div>
            </div>
        </div>
    }
</div>
