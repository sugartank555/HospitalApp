@model HospitalApp.Models.MedicalTest
@{
    Layout = "~/Areas/Dashboard/Views/Shared/_DashboardLayout.cshtml";
}
<h2>Chi tiết xét nghiệm</h2>
<dl class="row">
    <dt class="col-sm-3">Tên</dt>
    <dd class="col-sm-9">@Model.Name</dd>
    <dt class="col-sm-3">Thời gian</dt>
    <dd class="col-sm-9">@Model.TestTime</dd>
    <dt class="col-sm-3">Tổng tiền</dt>
    <dd class="col-sm-9">@Model.TotalPrice</dd>
    <dt class="col-sm-3">Hồ sơ</dt>
    <dd class="col-sm-9">@Model.MedicalRecordId</dd>
</dl>
<a class="btn btn-primary" asp-action="Edit" asp-route-id="@Model.Id">Sửa</a>
<a class="btn btn-secondary" asp-action="Index">Về danh sách</a>
